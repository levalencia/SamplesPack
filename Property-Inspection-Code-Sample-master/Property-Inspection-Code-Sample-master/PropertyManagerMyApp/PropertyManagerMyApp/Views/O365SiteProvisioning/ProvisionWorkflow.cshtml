@{
    ViewBag.Title = "Provisioning Incident Workflow, Incident Workflow Tasks list and Incident Workflow History list";
}


@using (Html.BeginForm("ProvisionWorkflow", "O365SiteProvisioning", FormMethod.Post))
{
    <div class="row">
        <h2>@ViewBag.Title</h2>

        @Html.ValidationSummary(true)

        <input class="btn btn-default" type="submit" value="Populate" />
    </div>

    <div class="row">
        <h4>@ViewBag.Message</h4>
    </div>
}
